{
	"name": "rust-api",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "libs/backend/rust-api/src",
	"targets": {
		"build": {
			"cache": true,
			"executor": "nx:run-commands",
			"outputs": [],
			"options": {
				"cwd": ".",
				"command": "cargo check -p rust_api"
			}
		},
		"test": {
			"cache": true,
			"executor": "nx:run-commands",
			"outputs": [],
			"options": {
				"cwd": ".",
				"command": "cargo test -p rust_api"
			},
			"configurations": {
				"production": {
					"command": "cargo test -p rust_api --release"
				}
			}
		},
		"lint": {
			"cache": true,
			"executor": "nx:run-commands",
			"options": {
				"cwd": ".",
				"command": "cargo clippy -p rust_api -- -D warnings"
			}
		}
	},
	"tags": []
}