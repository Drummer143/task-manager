{
	"name": "migrator",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "libs/backend/migrator/src",
	"targets": {
		"build": {
			"executor": "@monodon/rust:check",
			"outputs": ["{options.target-dir}"],
			"options": {
				"target-dir": "dist/target/migrator"
			}
		},
		"build-cli": {
			"executor": "@monodon/rust:build",
			"outputs": ["{options.target-dir}"],
			"options": {
				"target-dir": "dist/target/migrator-cli",
				"bin": "migrator"
			}
		},
		"test": {
			"cache": true,
			"executor": "@monodon/rust:test",
			"outputs": ["{options.target-dir}"],
			"options": {
				"target-dir": "dist/target/migrator"
			},
			"configurations": {
				"production": {
					"release": true
				}
			}
		},
		"lint": {
			"cache": true,
			"executor": "@monodon/rust:lint",
			"outputs": ["{options.target-dir}"],
			"options": {
				"target-dir": "dist/target/migrator"
			}
		}
	},
	"tags": []
}
