{
	"name": "migrator",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "libs/backend/migrator/src",
	"targets": {
		"build": {
			"cache": true,
			"executor": "nx:run-commands",
			"outputs": [],
			"options": {
				"cwd": ".",
				"command": "cargo check -p migrator"
			}
		},
		"build-cli": {
			"executor": "nx:run-commands",
			"outputs": [],
			"options": {
				"cwd": ".",
				"command": "cargo build -p migrator --bin migrator --release"
			}
		},
		"test": {
			"cache": true,
			"executor": "nx:run-commands",
			"outputs": [],
			"options": {
				"cwd": ".",
				"command": "cargo test -p migrator"
			},
			"configurations": {
				"production": {
					"command": "cargo test -p migrator --release"
				}
			}
		},
		"lint": {
			"cache": true,
			"executor": "nx:run-commands",
			"options": {
				"cwd": ".",
				"command": "cargo clippy -p migrator -- -D warnings"
			}
		}
	},
	"tags": []
}
