{
	"name": "task-manager",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/frontend/task-manager/src",
	"root": "apps/frontend/task-manager",
	"projectType": "application",
	"tags": [],
	"// targets": "to see all targets run: nx show project task-manager --web",
	"targets": {
		"e2e": {
			"executor": "@nx/playwright:playwright",
			"outputs": ["{workspaceRoot}/dist/.playwright/apps/frontend/task-manager"],
			"options": {
				"config": "{projectRoot}/playwright.config.ts"
			}
		},
		"lint:fix": {
			"executor": "nx:run-commands",
			"options": {
				"commands": ["eslint --fix {projectRoot}/**/*.{ts,tsx,js,jsx}"]
			}
		},
		"test": {
			"options": {
				"command": "echo test"
			}
		}
	}
}
